<% provide :title, 'Kiosk Selection' %>
<section class="container text-center">
    <h1>Kiosk</h1>
    <p>Choose a space to sign in to</p>
    <div class="list-group">
        <% @spaces.each do |name, id| %>
            <%= link_to name, kiosk_path(id), class: 'list-group-item list-group-item-action' %>
        <% end %>
    </div>
</section>
